pipeline {
    agent any

    stages {
        stage('Dev') {
            steps {
                echo 'starting Dev stage'
                echo 'Dev completed'
            }
        }
        stage('Build') {
            steps {
                echo 'Build stage started'
                echo 'Build completed'
            }
        }
        stage('Test') {
            steps {
                echo 'Test stage started'
                echo 'Test completed'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy stage started'
                echo 'Deploy completed'
            }
        }
        stage('Release') {
            steps {
                echo 'Release stage started'
                echo 'Deploy completed'
            }
        }
    }
}
